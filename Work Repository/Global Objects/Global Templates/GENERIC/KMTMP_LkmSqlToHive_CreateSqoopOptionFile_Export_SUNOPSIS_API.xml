# 
# SQOOP Option file for export
#
export


#############################################################
# Hadoop generic options
#############################################################

{# IF ($[isToTeradata] == 'true' AND ($[useGeneric] == 'true')) #}<?
//==========================================================
//===================== to Teradata ========================
//==========================================================
?>
-Dsqoop.export.records.per.statement=1
{# ENDIF #}

<?if (useCredStore) {?>
-D
hadoop.security.credential.provider.path=<?=credentialProviderPath?>
<?}?> 

<$=odiSqoopJars$>

$[option_EXTRA_HADOOP_CONF_PROPERTIES]


#############################################################
# SQOOP options
#############################################################

{# IF ($[useGeneric] == 'true') #}
--driver
$[destJavaDriver]
{# ENDIF #}

--connect
$[destJavaUrl]

--username
$[destUserName]

<?if (useCredStore) {?>
--password-alias
<?=passwordAlias?>
<?} else {?>
--password
$[destPassword]
<?}?>

--num-mappers
$[option_SQOOP_PARALLELISM]

--table
$[sqoopTargetTable]


{# IF ($[isFromFile] == 'true') #}<?
//==========================================================
//===================== from File ==========================
//==========================================================
?>

--export-dir
<$=sourceDir$>
--input-fields-terminated-by
'$[fieldSeparator]'
--input-lines-terminated-by
'$[rowSeparator]'
#--input-enclosed-by
#--input-escaped-by
#--input-optionally-enclosed-by
{# ENDIF #}

{# IF ($[isFromHive] == 'true' OR $[isFromHBase] == 'true' ) #}<?
//==========================================================
//===================== from Hive/HBase ====================
//==========================================================
?>
--export-dir
<$=sourceDir$>
--input-fields-terminated-by
'\01'
--input-lines-terminated-by
'\n'
#--input-enclosed-by
#--input-escaped-by
#--input-optionally-enclosed-by
--input-null-string
'\\N'
--input-null-non-string
'\\N'

{# ENDIF #}

$[option_EXTRA_SQOOP_CONF_PROPERTIES]

#############################################################
# Extra connector specific options
#############################################################
--

{# IF ($[isToTeradata] == 'true' AND ($[useGeneric] == 'false') #}<?
//==========================================================
//===================== to Teradata ========================
//==========================================================
?>
--staging-database
$[destWorkSchemaName]
--staging-table
$[teradataStageTableShortName]
--output-method
$[option_TERADATA_OUTPUT_METHOD]
{# ENDIF #}

{# IF ($[isToMsSql] == 'true' AND ($[useGeneric] == 'false') #}<?
//==========================================================
//===================== to MSSQL ===========================
//==========================================================
?>
--schema
<$=sqoopTargetSchema$>
{# ENDIF #}


$[option_EXTRA_SQOOP_CONNECTOR_CONF_PROPERTIES]
